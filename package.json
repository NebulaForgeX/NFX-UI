{
  "name": "nfx-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Unified frontend UI library for the NFX product family: shared React components, design tokens, theme variables, and layout primitives for a consistent look and behavior across NFX console and other apps.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "link": "npm run build && npm link",
    "prepublishOnly": "npm run build",
    "lint:code-format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,cjs,mjs,json,md}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,cjs,mjs,json,md}\"",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "nfx",
    "design-system"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@gsap/react": "^2.1.2",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@tanstack/react-query": "^5.90.21",
    "async-retry": "^1.3.3",
    "axios": "^1.13.5",
    "axios-case-converter": "^1.1.1",
    "buffer": "^6.0.3",
    "eventemitter3": "^5.0.4",
    "google-libphonenumber": "^3.2.44",
    "gsap": "^3.14.2",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "lenis": "^1.3.17",
    "lucide-react": "^0.575.0",
    "motion": "^12.34.3",
    "postprocessing": "^6.38.3",
    "react": "^19.2.4",
    "react-datepicker": "^9.1.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.2",
    "react-i18next": "^16.5.4",
    "react-intersection-observer": "^10.0.3",
    "react-pro-sidebar": "^1.1.0",
    "react-router-dom": "^7.13.1",
    "recharts": "^3.7.0",
    "three": "^0.183.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.183.1",
    "@vitejs/plugin-react": "^5.1.4",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.2",
    "eslint-plugin-unused-imports": "^4.4.1",
    "globals": "^17.3.0",
    "prettier": "^3.8.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.1",
    "vite": "^8.0.0-beta.13",
    "vite-plugin-dts": "^4.5.4"
  },
  "overrides": {
    "vite": "^8.0.0-beta.13",
    "minimatch": "^10.2.4"
  }
}
